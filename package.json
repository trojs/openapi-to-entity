{
    "name": "@trojs/openapi-to-entity",
    "version": "1.1.4",
    "description": "Convert Openapi schema to entity fields",
    "files": [
        "src/index.js"
    ],
    "main": "src/index.js",
    "scripts": {
        "lint": "eslint",
        "lint:report": "eslint src/*.js -f json -o report.json",
        "lint:fix": "eslint --fix",
        "lint:rules": "eslint --print-config file.js > eslintconfig.json",
        "test": "node --test --experimental-test-coverage --test-reporter=spec --test-reporter=lcov --test-reporter-destination=stdout --test-reporter-destination=./coverage/lcov.info",
        "test:watch": "node --watch --test --experimental-test-coverage --test-reporter=spec --test-reporter=lcov --test-reporter-destination=stdout --test-reporter-destination=./coverage/lcov.info",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --omit=dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/trojs/openapi-to-entity.git"
    },
    "keywords": [
        "openapi",
        "entity",
        "orm",
        "db"
    ],
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://trojs.org/"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/trojs/openapi-to-entity/issues"
    },
    "homepage": "https://github.com/trojs/openapi-to-entity#readme",
    "devDependencies": {
        "@trojs/lint": "^0.4.0",
        "@trojs/error": "^5.0.0",
        "eslint": "^9.15.0",
        "globals": "^16.0.0",
        "jscpd": "^4.0.0",
        "openapi-types": "^12.1.3"
    },
    "type": "module",
    "engines": {
        "node": ">= 20"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/w3nl"
    }
}
